<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
      <record id="zip_list_view" model="ir.ui.view">
          <field name="model">zip.list</field>
          <field name="type">tree</field>
          <field name="arch" type="xml">
            <tree>
              <field name="zip"></field>
              <field name="team"></field>
            </tree>
          </field>
      </record>
      <record id="zip_list_view_form" model="ir.ui.view">
          <field name="model">zip.list</field>
          <field name="type">form</field>
          <field name="arch" type="xml">
            <form>
              <field name="zip"></field>
              <field name="team"></field>
            </form>
          </field>
      </record>
    </data>
</odoo> 