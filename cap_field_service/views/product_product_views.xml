<?xml version="1.0" ?>
<odoo>
    <record id="product_product_view_form_inherit_fields" model="ir.ui.view">
        <field name="name">product.product.view.form.inherit.fields</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="industry_fsm_sale.view_product_product_kanban_material"/>
        <field name="arch" type="xml">
            <field name="currency_id" position="after">
                <field name="user_warehouse" invisible="1"/>
                <field name="user_warehouse_product" invisible="1"/>
                <field name="store_qty_available" invisible="1"/>
            </field>
        </field>
    </record>
</odoo>
